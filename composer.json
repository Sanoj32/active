{
  "name": "sanoj32/active",
  "description": "The helper class for Laravel applications to get active class base on current route",
  "keywords": [
    "laravel",
    "active",
    "routing"
  ],
  "license": "MIT",
  "homepage": "https://www.hieule.info/tag/laravel-active/",
  "authors": [
    {
      "name": "Hieu Le",
      "email": "letrunghieu.cse09@gmail.com",
      "homepage": "https://www.hieule.info"
    },
    {
      "name": "Sanoj Raj Shrestha",
      "email": "sanoj.shrestha.13@gmail.com",
      "homepage": "https://www.linkedin.com/in/sanoj-shrestha-5222ab1ba/"
    }
  ],
  "require": {
    "php": "^8.1",
    "laravel/framework": "^12.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0",
    "orchestra/testbench": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "HieuLe\\Active\\": "src/",
      "HieuLe\\ActiveTest\\": "tests/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "HieuLe\\Active\\ActiveServiceProvider"
      ],
      "aliases": {
        "Active": "HieuLe\\Active\\Facades\\Active"
      }
    }
  }
}
